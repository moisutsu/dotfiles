[user]
	name = moisutsu
	email = moisutsu@gmail.com
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
[merge]
	tool = vim \"$MERGED\"
[ghq]
	root = ~/ghq
[commit]
	template = ~/.commit_template
[credential]
	helper = cache
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main
[fetch]
	prune = true
