[user]
	name = moisutsu
	email = moisutsu@gmail.com
[core]
	editor = code --wait
	excludesfile = ~/.gitignore_global
[merge]
	tool = code --wait \"$MERGED\"
[ghq]
	root = ~/ghq
[commit]
	template = ~/.commit_template
[credential]
	helper = cache
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
